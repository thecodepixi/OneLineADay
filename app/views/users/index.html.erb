<%= render 'flash' %>

<%= link_to "Create An Account", new_user_path %>

<h3>Log In:</h3>
<%= form_with(url: "/login", method: "post") do %>
  <div class="form-group">
    <%= label_tag "Username:"%>
    <%= text_field_tag :username %>
  </div>
  <div class="form-group">
    <%= label_tag "Password:" %>
    <%= password_field_tag :password %>
  </div>
    <%= submit_tag "Log In"%>
<% end %>

  <p>Try <%= link_to 'Signing in with the developer strategy', '/auth/developer' %>.</p>
  <p>Try <%= link_to 'Signing in with the GitHub strategy', '/auth/github', method: :post %>.</p>
  <p>Try <%= link_to 'Signing in with the Facebook strategy', '/auth/facebook', method: :post %>.</p>



